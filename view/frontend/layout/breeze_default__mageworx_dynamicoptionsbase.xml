        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="product" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="dynamicOptionsBase_dependencies" xsi:type="string">
                                Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionsbase/dynamicoptions-dependencies
                            </item>
                            <item name="dynamicOptionsBase_component" xsi:type="string">
                                MageWorx_DynamicOptionsBase/js/dynamicOptions
