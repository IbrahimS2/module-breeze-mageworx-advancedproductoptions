                            <item name="dynamicOptionsBase_register" xsi:type="string">
                                Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionsbase/dynamicoptions-register
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
